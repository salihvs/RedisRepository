<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Authors>Serkan Yazıcıoğlu</Authors>
    <Company>Serkan Yazıcıoğlu</Company>
    <Description>.Net standard repository library for Redis.</Description>
    <RepositoryType>git</RepositoryType>
    <PackageProjectUrl>https://github.com/serkanyazicioglu/RedisRepository</PackageProjectUrl>
    <RepositoryUrl>https://github.com/serkanyazicioglu/RedisRepository</RepositoryUrl>
    <PackageTags>repository, nhea, redis, cache</PackageTags>
    <PackageReleaseNotes>Added connection pooling.</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>2.0.9</Version>
    <AssemblyVersion>2.0.9.0</AssemblyVersion>
    <FileVersion>2.0.9.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Nhea" Version="2.0.0.10" />
    <PackageReference Include="StackExchange.Redis" Version="2.2.62" />
    <PackageReference Include="System.Runtime.Caching" Version="5.0.0" />
    <PackageReference Include="System.Text.Json" Version="5.0.2" />
  </ItemGroup>

</Project>
